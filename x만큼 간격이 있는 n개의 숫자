#include <string>
#include <vector>

using namespace std;
 //입력받는건 정수x와 자연수n
    //출력값은 정수x에서 시작해서 x씩 증가하는 n개의 숫자의 배열

vector<long long> solution(int x, int n) {
     vector<long long> answer(n); //크기가 n인 벡터배열 생성

    for(int i=0;i<n;i++){
       answer[i]=(long long)x*(i+1);


    }


    return answer;
}
