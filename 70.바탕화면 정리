#include <string>
#include <vector>
#include <algorithm>
using namespace std;

vector<int> solution(vector<string> wallpaper) {
    int n = wallpaper.size();
    int m = wallpaper[0].size();

    int minRow = 1e9, minCol = 1e9;
    int maxRow = -1, maxCol = -1;

    // 모든 '#' 위치 탐색
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < m; j++) {
            if (wallpaper[i][j] == '#') {
                minRow = min(minRow, i);
                minCol = min(minCol, j);
                maxRow = max(maxRow, i);
                maxCol = max(maxCol, j);
            }
        }
    }

    // 끝점은 격자점 기준으로 +1
    return {minRow, minCol, maxRow + 1, maxCol + 1};
}
