#include <string>
using namespace std;

int solution(string t, string p) {
    int count = 0;
    int pLen = p.length();
    long long pVal = stoll(p); // p를 정수로 변환 (stoll은 큰 수 처리 가능)

    for (int i = 0; i <= t.length() - pLen; ++i) {
        string sub = t.substr(i, pLen); // 부분 문자열 추출
        long long subVal = stoll(sub);  // 정수로 변환

        if (subVal <= pVal) {
            ++count;
        }
    }

    return count;
}
