#include <string>
#include <vector>

using namespace std;

string solution(int a, int b) {
    int month_days[12] = {31,29,31,30,31,30,31,31,30,31,30,31};
    string week[7] = {"FRI","SAT","SUN","MON","TUE","WED","THU"};
    
    int days = 0;
    for(int i = 0; i < a-1; i++) {
        days += month_days[i];
    }
    days += (b - 1);
    
    return week[days % 7];
}
