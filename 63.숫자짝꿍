#include <string>
#include <vector>
#include <algorithm>
using namespace std;

string solution(string X, string Y) {
    vector<int> countX(10, 0), countY(10, 0);

    // 각 숫자 등장 횟수 세기
    for (char c : X) countX[c - '0']++;
    for (char c : Y) countY[c - '0']++;

    string result = "";

    // 9부터 0까지 공통으로 등장한 개수만큼 추가
    for (int i = 9; i >= 0; i--) {
        int common = min(countX[i], countY[i]);
        result.append(common, i + '0');
    }

    if (result.empty()) return "-1";       // 공통 숫자 없음
    if (result[0] == '0') return "0";      // 전부 0으로만 구성됨
    return result;
}
