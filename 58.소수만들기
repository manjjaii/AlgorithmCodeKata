#include <iostream>
#include <vector>
#include <cmath>
using namespace std;

// 소수 판별 함수
bool isPrime(int n) {
    if (n < 2) return false;
    for (int i = 2; i <= sqrt(n); i++) {
        if (n % i == 0) return false;
    }
    return true;
}

int solution(vector<int> nums) {
    int count = 0;
    int n = nums.size();

    for (int i = 0; i < n - 2; i++) {
        for (int j = i + 1; j < n - 1; j++) {
            for (int k = j + 1; k < n; k++) {
                int sum = nums[i] + nums[j] + nums[k];
                if (isPrime(sum)) count++;
            }
        }
    }
    return count;
}

// 사용 예시
int main() {
    vector<int> nums = {1, 2, 3, 4};
    cout << solution(nums) << endl;  // 출력: 1 (1+2+4=7이 소수)
}
