#include <string>
#include <vector>
#include <algorithm>

using namespace std;

int solution(vector<vector<int>> sizes) {
    int maxWidth = 0;
    int maxHeight = 0;

    for (auto card : sizes) {
        int w = max(card[0], card[1]); // 항상 큰 쪽을 가로
        int h = min(card[0], card[1]); // 작은 쪽을 세로

        maxWidth = max(maxWidth, w);   // 가장 큰 가로 길이 갱신
        maxHeight = max(maxHeight, h); // 가장 큰 세로 길이 갱신
    }

    return maxWidth * maxHeight; // 최소 지갑 크기
}
