#include <string>
#include <vector>
#include <queue>

using namespace std;

vector<int> solution(int k, vector<int> score) {
    vector<int> answer;
    priority_queue<int, vector<int>, greater<int>> pq; // 최소 힙

    for (int s : score) {
        pq.push(s);
        if (pq.size() > k) {
            pq.pop(); // k개만 유지
        }
        answer.push_back(pq.top()); // 현재 명예의 전당 최하위 점수
    }

    return answer;
}
