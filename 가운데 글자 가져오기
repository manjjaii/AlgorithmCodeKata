#include <string>
#include <vector>

using namespace std;

string solution(string s)
{
    int len = s.length();
    if(len%2==0)
    {
      return s.substr(len/2-1,2);  //substr함수는 문자열의 일부분을 잘라서 새 문자열로 반환하는 함수, 여기선 가운데 글자 두글자를 반환
        
    }
    else
    {
        return s.substr(len/2,1);//여기선 가운데 한글자를 반환
    }
  
}
